{
  "name": "@sentio/runtime",
  "version": "2.0.0-development",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "bin": {
    "processor-runner": "./lib/processor-runner.js"
  },
  "files": [
    "{lib,src}",
    "!**/*.test.{js,ts}",
    "!{lib,src}/tests"
  ],
  "scripts": {
    "build": "pnpm tsc --noEmit && pnpm bundle",
    "build:all": "pnpm --filter=$(node -p \"require('./package.json').name\")... build",
    "bundle": "tsup --config src/tsup.config.ts",
    "compile": "tsc",
    "run": "tsx src/processor-runner.ts --log-format=json",
    "run-benchmark": "tsx src/decode-benchmark.ts",
    "start_js": "tsx ./lib/processor-runner.js $PWD/../../debug/dist/lib.js",
    "start_ts": "tsx src/processor-runner.ts --debug --chains-config chains-config.json $PWD/../../examples/x2y2/src/processor.ts",
    "test": "glob -c 'tsx --test' '**/*.test.ts'"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.205.0",
    "@opentelemetry/exporter-prometheus": "^0.205.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@sentio/protos": "workspace:*",
    "commander": "^14.0.0",
    "ethers": "npm:@sentio/ethers@6.13.1-patch.6",
    "fs-extra": "^11.2.0",
    "ix": "^7.0.0",
    "long": "^5.2.3",
    "lru-cache": "^11.1.1",
    "nice-grpc": "^2.1.12",
    "nice-grpc-client-middleware-retry": "^3.1.6",
    "nice-grpc-common": "^2.0.2",
    "nice-grpc-error-details": "^0.2.4",
    "nice-grpc-opentelemetry": "^0.1.18",
    "nice-grpc-prometheus": "^0.2.2",
    "piscina": "5.1.3",
    "protobufjs": "^7.5.4",
    "rxjs": "^7.8.1",
    "utility-types": "^3.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@types/fs-extra": "^11.0.4"
  },
  "engines": {
    "node": ">=22"
  }
}
