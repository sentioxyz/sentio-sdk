{
  "name": "@sentio/sdk",
  "license": "Apache-2.0",
  "version": "1.0.0-development",
  "scripts": {
    "compile_target": "yarn tsc -b src/target-ethers-sentio/tsconfig.json",
    "compile": "tsc -p . && cp src/cli/webpack.config.js lib/cli && cp src/utils/*.csv lib/utils",
    "build": "yarn gen && yarn compile",
    "start_ts": "ts-node --files src/processor-runner.ts ../examples/x2y2/src/processor.ts",
    "start": "ts-node --files src/processor-runner.ts ../examples/aptos/dist/lib.js",
    "start_js": "node lib/processor-runner.js ../examples/aptos/dist/lib.js",
    "ts-start": "ts-node --files src/processor-runner.ts ./src/anyswap/src/processor.ts",
    "cli": "ts-node src/cli/cli.ts",
    "test": "jest",
    "gen": "yarn compile_target && yarn gen_bultin_evm && yarn gen_bultin_others && yarn gen_test_abi",
    "gen_test_abi": "ts-node src/test-abi-code-gen.ts src/tests/abis src/tests/types",
    "gen_bultin_evm": "typechain --target lib/target-ethers-sentio --out-dir src/builtin/internal 'src/abis/*.json'",
    "gen_bultin_others": "ts-node src/test-abi-code-gen.ts src/abis src/builtin",
    "postbuild": "echo || clear-package-json package.json -o ./lib/package.json && cp release.config.js ./lib",
    "docs": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "@ethersproject/providers": "~5.7.0",
    "@project-serum/anchor": "^0.25.0",
    "@solana/web3.js": "^1.47.3",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/express": "^4.17.14",
    "@types/node-fetch": "^2.6.2",
    "aptos-sdk": "npm:aptos@^1.3.16",
    "bignumber.js": "^9.1.0",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "csv-parse": "^5.3.0",
    "ethers": "~5.7.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs-extra": "^10.1.0",
    "google-protobuf": "^3.15.8",
    "js-yaml": "^4.1.0",
    "nice-grpc": "^2.0.0",
    "node-fetch": "2",
    "p-queue": "^6.6.2",
    "ts-loader": "^9.3.0",
    "typechain": "^8.0.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/expect": "^24.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/google-protobuf": "^3.15.6",
    "@types/js-yaml": "^4.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.0.4",
    "chai": "^4.3.6",
    "clean-publish": "^4.0.1",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "jest": "^29.0.3",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.0"
  },
  "bin": {
    "processor-runner": "./lib/processor-runner.js",
    "sentio": "./lib/cli/cli.js"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "files": [
    "{lib,src,templates}",
    "!{lib,src}/tests",
    "!**/*.test.{js,ts}"
  ],
  "engines": {
    "node": ">=16"
  }
}
